<header class="header">
  <div class="navbar navbar-fixed-top navbar-inverse">
    <div class="navbar-inner">
      <div class="container">
        <ul class="nav">
          <%= link_to "[PBL][MP]", root_path, class: "brand" %>
          <% if current_member %>
            <li><%= link_to "Tabling Schedule", tabling_slots_url %></li>
            <li><%= link_to "Events", events_url %></li>
            <li><%= link_to "Reimbursements", reimbursements_url %></li>
            <li><%= link_to "Commitment Calendars", commitment_calendars_url %></li>
          <% end %>
        </ul>
        <ul class="nav pull-right">
          <% if current_member %>
            <li><%= link_to current_member.name, "#" %></li>
            <li><%= link_to "Sign out", signout_path, method: :delete %></li>
          <% else %>
            <li><%= link_to "Sign in", root_path %></li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</header>
