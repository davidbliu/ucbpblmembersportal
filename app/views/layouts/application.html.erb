<!DOCTYPE html>
<html>
<head>
  <title>Ucbpblmembersportal</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= stylesheet_link_tag    "http://fonts.googleapis.com/css?family=Raleway:300,400,700' rel='stylesheet" %>
  <%= javascript_include_tag "application" %>

  <%= csrf_meta_tags %>
</head>
  <body>
    <%= render "layouts/header" %>

    <% if not flash.empty? %>
      <section class="flash">
        <ul>
          <% flash.each do |key, value| %>
            <li class="alert alert-<%= key %>"><%= value %></li>
          <% end %>
        </ul>
      </section>
    <% end %>

    <%= yield %>

  </body>
</html>
