<%= form_for reimbursement, html: { class: "form" } do |f| %>
  <%= render "layouts/member_autocomp_form" %>

  <%= f.label :amount, "Amount: $" %>
  <%= f.text_field :amount %>

  <%= f.label :for %>
  <%= f.text_field :details %>

  <p><%= f.submit "Add", class: "btn btn-primary" %></p>
<% end %>
